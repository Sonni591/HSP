<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane fx:id="borderPane" prefHeight="311.0" prefWidth="551.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="de.oth.hsp.clsp.view.BatchProcessingDialogController">
	<top>
		<AnchorPane BorderPane.alignment="CENTER" />
	</top>
	<center>
		<AnchorPane prefHeight="338.0" prefWidth="614.0" BorderPane.alignment="CENTER">
			<children>
            <Button fx:id="btnChooseDatFiles" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#onActionChooseDatFiles" text="Eingabedateien auswählen" />
						<Button fx:id="btnStart" defaultButton="true" layoutX="398.0" layoutY="271.0" mnemonicParsing="false" onAction="#onActionStart" text="Starten" />
						<Button fx:id="btnClose" cancelButton="true" layoutX="468.0" layoutY="271.0" mnemonicParsing="false" onAction="#onActionClose" text="Schließen" />
            <TextField fx:id="txtDestination" layoutX="139.0" layoutY="225.0" prefHeight="25.0" prefWidth="384.0" />
            <ListView fx:id="lvChoosenDatFiles" layoutX="14.0" layoutY="50.0" prefHeight="154.0" prefWidth="509.0" />
            <Button fx:id="btnChooseDestination" layoutX="14.0" layoutY="225.0" mnemonicParsing="false" onAction="#onActionChooseDestination" text="Zielort auswählen" />
            <ProgressIndicator fx:id="piBatchProcessingProgress" layoutX="253.0" layoutY="116.0" visible="false" />
			</children>
		</AnchorPane>
	</center>
</BorderPane>
